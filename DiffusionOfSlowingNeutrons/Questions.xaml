<Window x:Class="NuclearProject.Questions"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NuclearProject"
        mc:Ignorable="d"
        Title="Questions" Height="700" Width="700">
    <Grid>
        <ScrollViewer>
            <StackPanel Name="QuestionList">
                <WrapPanel HorizontalAlignment="Center">
                    <Label HorizontalAlignment="Center" VerticalAlignment="Center" >Тестирование</Label>
                    <Button x:Name="submitTest" Click="SubmitTest"
                    Content="Проверить" Height="30" MinWidth="105" Margin="2,2,2,2" HorizontalAlignment="Center" VerticalAlignment="Center" Background="CornflowerBlue"/>
                </WrapPanel>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Window>
