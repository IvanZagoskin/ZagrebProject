<Window x:Class="NuclearProject.Questions"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NuclearProject"
        mc:Ignorable="d"
        Title="Тестирование" Height="800" Width="800">
    <Grid>
        <ScrollViewer>
            <WrapPanel HorizontalAlignment="Stretch">
                <WrapPanel Name="QuestionList" HorizontalAlignment="Stretch"></WrapPanel>
                <StackPanel Width="700">
                    <Button x:Name="submitTest" Click="SubmitTest"
                    Content="Проверить" Height="30" MinWidth="105" Margin="10,10,10,10" HorizontalAlignment="Center" VerticalAlignment="Center" Background="CornflowerBlue"/>
                </StackPanel>
            </WrapPanel>
        </ScrollViewer>
    </Grid>
</Window>
